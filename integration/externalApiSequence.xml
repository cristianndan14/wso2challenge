<sequence name="externalApiSequence" xmlns="http://ws.apache.org/ns/synapse">
    <property name="OUT_ONLY" value="true"/>
    <send>
        <endpoint>
            <http method="GET" uri-template="https://apis.digital.gob.cl/fl/feriados/2023"/>
        </endpoint>
    </send>
    <!-- Procesar la respuesta del servicio -->
    <log level="full"/>
    <property name="RESPONSE" expression="get-property('transport', 'RESPONSE')"/>
    <log level="full">
        <property name="ResponsePayload" expression="$ctx:RESPONSE"/>
    </log>
    <payloadFactory media-type="json">
        <format>$1</format>
        <args>
            <arg evaluator="xml" expression="$ctx:RESPONSE"/>
        </args>
    </payloadFactory>
    <log level="full"/>
    <respond/>
</sequence>
